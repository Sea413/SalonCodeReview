<h1>Sadness page</h1>

<form action="/Stylist/ViewClient" method="post">
  <label for="name">Name: </label>
  <input type="text" name="name">
  <br>  <br>
  <select name="stylist-id">
     @foreach (var stylist in Model)
     {
       <option value="@stylist.GetId()">@stylist.GetName()</option>
     }
   </select>
  <button type="submit" name="button">Click here to add your stylist!</button>
</form>
<br><br>
<a href="/">Go Home!</a>
